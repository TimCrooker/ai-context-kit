{
  "name": "@timothycrooker/ai-context-config",
  "version": "0.2.1",
  "description": "Shared configuration presets for ai-context-kit",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./tsconfig/base": "./tsconfig/base.json"
  },
  "files": [
    "dist",
    "tsconfig"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "clean": "rm -rf dist",
    "lint": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --passWithNoTests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimCrooker/ai-context-kit.git"
  },
  "bugs": {
    "url": "https://github.com/TimCrooker/ai-context-kit/issues"
  },
  "homepage": "https://github.com/TimCrooker/ai-context-kit#readme"
}
