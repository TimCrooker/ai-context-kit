{
  "name": "ai-context-kit",
  "version": "0.1.0",
  "private": true,
  "description": "Codex + Claude context generation toolkit",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimCrooker/ai-context-kit.git"
  },
  "bugs": {
    "url": "https://github.com/TimCrooker/ai-context-kit/issues"
  },
  "homepage": "https://github.com/TimCrooker/ai-context-kit#readme",
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "build": "pnpm -r build",
    "clean": "pnpm -r clean",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "typecheck": "pnpm -r typecheck",
    "ai-context": "node packages/cli/dist/index.js",
    "format": "prettier --check \"**/*.{ts,tsx,js,json,md,yml,yaml}\"",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,json,md,yml,yaml}\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.6",
    "@types/node": "^22.13.1",
    "prettier": "^3.5.0",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  }
}
